[
  {
    "name": "Javascript",
    "dir": "../../Benchmarking/Javascript/",
    "setup": {
      "args": ["npm install", "tsc"]
    },
    "run": {
      "args": ["node ./js/fib.js 40"]
    }
  },
  {
    "name": "Rust",
    "dir": "../../Benchmarking/Rust/fib/",
    "setup": {
      "args": ["cargo build --release"]
    },
    "run": {
      "args": ["./target/release/fib 40"]
    }
  },
  {
    "name": "c++",
    "dir": "../../Benchmarking/C++/",
    "setup": {
      "args": ["g++ fib.cpp -o fib"]
    },
    "run": {
      "args": ["./fib 40"]
    }
  },
  {
    "name": "Go",
    "dir": "../../Benchmarking/Go/",
    "setup": {
      "args": ["go build main.go"]
    },
    "run": {
      "args": ["./main 40"]
    }
  },
  {
    "name": "Java",
    "dir": "../../Benchmarking/Java/",
    "setup": {
      "args": ["javac FibRecursive.java"]
    },
    "run": {
      "args": ["java FibRecursive 40"]
    }
  }
]
